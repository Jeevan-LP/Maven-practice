<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <!--
	* Initially all @BeforeTest will execute first, before executing all @Test methods
	 based on how many test cases we have
    * Eventually all @AfterTest will execute last, after executing all @Test methods
     based on how many test cases we have
    * We can create multiple test names 
    -->
  <test thread-count="5" name="Test1">
  	 <classes>
		<class name="Annotations.Methods_Classes_Testes_01"/>
     	<class name="Annotations.Methods_Classes_Testes_02"/>
     </classes>
  </test> 
  
  <!-- Not neccessary if we want execute same Test again then use this
  <test thread-count="5" name="Test2">
    <classes>
		<class name="Annotations.Methods_Classes_Testes_01"/>
     	<class name="Annotations.Methods_Classes_Testes_02"/>
    </classes>
  </test>  -->
  
</suite> <!-- Suite -->
